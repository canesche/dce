; ModuleID = 'benchmarks/test1.vssa.rbc'
source_filename = "benchmarks/test1.c"
target datalayout = "e-m:e-p270:32:32-p271:32:32-p272:64:64-i64:64-f80:128-n8:16:32:64-S128"
target triple = "x86_64-unknown-linux-gnu"

; Function Attrs: noinline nounwind uwtable
define dso_local i32 @foo() #0 {
entry:
  br label %for.cond

for.cond:                                         ; preds = %for.inc, %entry
  %sum.0 = phi i32 [ 0, %entry ], [ %add, %for.inc ]
  %i.0 = phi i32 [ 0, %entry ], [ %inc, %for.inc ]
  %cmp = icmp slt i32 %i.0, 100
  br i1 %cmp, label %for.body, label %for.cond.for.end_crit_edge

for.cond.for.end_crit_edge:                       ; preds = %for.cond
  br label %for.end

for.body:                                         ; preds = %for.cond
  %vSSA_sigma = phi i32 [ %i.0, %for.cond ]
  %add = add nsw i32 %sum.0, %vSSA_sigma
  %cmp1 = icmp sgt i32 %vSSA_sigma, 20
  br i1 %cmp1, label %if.then, label %if.end

if.then:                                          ; preds = %for.body
  br label %for.end

if.end:                                           ; preds = %for.body
  %vSSA_sigma1 = phi i32 [ %vSSA_sigma, %for.body ]
  br label %for.inc

for.inc:                                          ; preds = %if.end
  %inc = add nsw i32 %vSSA_sigma1, 1
  br label %for.cond

for.end:                                          ; preds = %for.cond.for.end_crit_edge, %if.then
  %sum.1 = phi i32 [ %add, %if.then ], [ %sum.0, %for.cond.for.end_crit_edge ]
  ret i32 %sum.1
}

